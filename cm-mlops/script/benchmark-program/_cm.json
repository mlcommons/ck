    "numactl": {
      "default_env": {
        "CM_ENABLE_NUMACTL": 1,
        "CM_NUMACTL_MEMBIND": "--localalloc"
      }
    },
    "numactl-interleave": {
      "default_env": {
        "CM_ENABLE_NUMACTL": 1,
        "CM_NUMACTL_MEMBIND": "--interleave=all"
      }
    },
    "profile": {
      "default_env": {
        "CM_ENABLE_PROFILING": 1
