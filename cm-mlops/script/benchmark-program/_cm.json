  "default_env": {
    "CM_ENABLE_NUMACTL": "0",
    "CM_ENABLE_PROFILING": "0"
  },
  "deps": [
    {
      "tags": "detect,cpu"
    }
  ],
  "tags": [
    "program",
    "benchmark",
    "benchmark-program"
  ],
